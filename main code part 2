      "cell_type": "code",
      "source": [
        "import numpy as np\n",
        "import pandas as pd\n",
        "import tensorflow as tf\n",
        "from sklearn.preprocessing import StandardScaler\n",
        "from sklearn.metrics import mean_absolute_error\n",
        "import matplotlib.pyplot as plt\n",
        "\n",
        "# 1. LOAD YOUR SAVED DATA\n",
        "print(\"1. Loading Data...\")\n",
        "X_img = np.load(\"/content/drive/MyDrive/COLAB DATASET/X_images_64.npy\").astype('float32') / 255.0\n",
        "df = pd.read_csv(\"/content/drive/MyDrive/COLAB DATASET/train_master_physics.csv\")\n",
        "\n",
        "# Align lengths\n",
        "min_len = min(len(X_img), len(df))\n",
        "X_img = X_img[:min_len]\n",
        "df = df.iloc[:min_len]\n",
        "\n",
        "# ---------------------------------------------------------\n",
        "# FIX 1: CHRONOLOGICAL SPLIT (The \"Time Traveler\" Fix)\n",
        "# ---------------------------------------------------------\n",
        "print(\"\\n2. Applying Chronological Split (No Shuffling)...\")\n",
        "# We split by TIME, not random chance.\n",
        "split_point = int(len(df) * 0.80) # 80% Past, 20% Future\n",
        "\n",
        "X_train_img = X_img[:split_point]\n",
        "X_test_img = X_img[split_point:]\n",
        "\n",
        "# We need the physics inputs too\n",
        "scaler = StandardScaler()\n",
        "# Fit ONLY on past data to avoid data leakage\n",
        "X_train_phys = scaler.fit_transform(df['GHI_limit'].iloc[:split_point].values.reshape(-1, 1))\n",
        "X_test_phys = scaler.transform(df['GHI_limit'].iloc[split_point:].values.reshape(-1, 1))\n",
        "\n",
        "y_train = df['target_k'].iloc[:split_point].values\n",
        "y_test = df['target_k'].iloc[split_point:].values\n",
        "\n",
        "print(f\"   Train Set: {len(y_train)} samples (Past)\")\n",
        "print(f\"   Test Set:  {len(y_test)} samples (Future)\")\n",
        "\n",
        "# ---------------------------------------------------------\n",
        "# RELOAD & RETRAIN (Fast Retrain on Honest Data)\n",
        "# ---------------------------------------------------------\n",
        "# Load architecture\n",
        "model = tf.keras.models.load_model(\"/content/drive/MyDrive/COLAB DATASET/final_physics_model.keras\", compile=False)\n",
        "model.compile(optimizer='adam', loss='mse', metrics=['mae'])\n",
        "\n",
        "print(\"   Retraining on Chronological Data (Fast Tuning)...\")\n",
        "model.fit(\n",
        "    [X_train_img, X_train_phys], y_train,\n",
        "    epochs=3, # Short fine-tuning is enough\n",
        "    batch_size=64,\n",
        "    verbose=0\n",
        ")\n",
        "print(\"   ✅ Model Retrained on 'Honest' Data.\")\n",
        "\n",
        "# Predict\n",
        "y_pred_raw = model.predict([X_test_img, X_test_phys]).flatten()\n",
        "\n",
        "# ---------------------------------------------------------\n",
        "# FIX 2: THERMODYNAMIC CORRECTION (The Physics Fix)\n",
        "# ---------------------------------------------------------\n",
        "print(\"\\n3. Applying Thermodynamic Correction...\")\n",
        "# Physics Rule: Panels lose ~0.4% efficiency per °C above 25°C.\n",
        "# Approximation: High GHI = High Temp.\n",
        "# Formula: Efficiency_Factor = 1 - 0.004 * (Estimated_Temp - 25)\n",
        "\n",
        "ghi_test = df['GHI_limit'].iloc[split_point:].values\n",
        "# Estimate Cell Temp: Ambient (25C) + Heating from Sun\n",
        "estimated_cell_temp = 25 + (ghi_test * 0.03)\n",
        "efficiency_loss = 1 - 0.004 * (estimated_cell_temp - 25)\n",
        "\n",
        "# Apply Correction\n",
        "y_pred_corrected = y_pred_raw * efficiency_loss\n",
        "\n",
        "mae_raw = mean_absolute_error(y_test, y_pred_raw)\n",
        "mae_phys = mean_absolute_error(y_test, y_pred_corrected)\n",
        "\n",
        "print(f\"   Raw Model Error (MAE):     {mae_raw:.4f}\")\n",
        "print(f\"   Physics-Corrected Error:   {mae_phys:.4f}\")\n",
        "if mae_phys < mae_raw:\n",
        "    print(\"   🚀 Physics Correction Improved Accuracy!\")\n",
        "else:\n",
        "    print(\"   (Physics correction minimal - likely mild weather days)\")\n",
        "\n",
        "# ---------------------------------------------------------\n",
        "# FIX 3: RAMP DETECTION INDEX (The Safety Fix)\n",
        "# ---------------------------------------------------------\n",
        "print(\"\\n4. Calculating Grid Safety (Ramp Detection Index)...\")\n",
        "\n",
        "# Define a \"Ramp\" as a change > 15% in power between steps\n",
        "ramp_threshold = 0.15\n",
        "actual_ramps = np.abs(np.diff(y_test)) > ramp_threshold\n",
        "pred_ramps = np.abs(np.diff(y_pred_corrected)) > ramp_threshold\n",
        "\n",
        "# We look at the overlap (Did we predict the ramps that happened?)\n",
        "# Note: diff reduces array size by 1\n",
        "true_positives = np.sum(actual_ramps & pred_ramps)\n",
        "total_real_ramps = np.sum(actual_ramps)\n",
        "\n",
        "if total_real_ramps > 0:\n",
        "    rdi = true_positives / total_real_ramps\n",
        "else:\n",
        "    rdi = 0\n",
        "\n",
        "print(f\"   Total Major Ramp Events: {total_real_ramps}\")\n",
        "print(f\"   Correctly Predicted:     {true_positives}\")\n",
        "print(f\"   🏆 Ramp Detection Index (RDI): {rdi:.2f} (Target > 0.60)\")\n",
        "\n",
        "# ---------------------------------------------------------\n",
        "# VISUAL PROOF (The Chart)\n",
        "# ---------------------------------------------------------\n",
        "\n",
        "# Let's plot a specific ramp event to show the \"Correction\" working\n",
        "plt.figure(figsize=(10, 5))\n",
        "subset = slice(100, 150) # Zoom in\n",
        "plt.plot(y_test[subset], 'k-', label='Actual Power', linewidth=2)\n",
        "plt.plot(y_pred_raw[subset], 'r--', label='Raw AI', alpha=0.6)\n",
        "plt.plot(y_pred_corrected[subset], 'g-', label='Physics-Corrected', linewidth=2)\n",
        "plt.title('Impact of Physics Correction on Ramp Prediction')\n",
        "plt.legend()\n",
        "plt.show()"
      ],
