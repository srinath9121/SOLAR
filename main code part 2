      "cell_type": "code",
      "source": [
        "import tensorflow as tf\n",
        "import numpy as np\n",
        "import pandas as pd\n",
        "from sklearn.model_selection import train_test_split\n",
        "from sklearn.preprocessing import StandardScaler\n",
        "\n",
        "# 1. LOAD DATA (Reload to be safe)\n",
        "print(\"1. Loading Data...\")\n",
        "X_img = np.load(\"/content/drive/MyDrive/COLAB DATASET/X_images_64.npy\").astype('float32') / 255.0\n",
        "df = pd.read_csv(\"/content/drive/MyDrive/COLAB DATASET/train_master_physics.csv\")\n",
        "\n",
        "# Align\n",
        "min_len = min(len(X_img), len(df))\n",
        "X_img = X_img[:min_len]\n",
        "df = df.iloc[:min_len]\n",
        "\n",
        "# Prepare Inputs\n",
        "scaler = StandardScaler()\n",
        "X_phys = scaler.fit_transform(df[['GHI_limit']].values)\n",
        "y = df['target_k'].values\n",
        "\n",
        "# Split (Chronological to prevent leakage)\n",
        "split = int(len(df) * 0.8)\n",
        "X_img_train, X_img_test = X_img[:split], X_img[split:]\n",
        "X_phys_train, X_phys_test = X_phys[:split], X_phys[split:]\n",
        "y_train, y_test = y[:split], y[split:]\n",
        "\n",
        "# 2. DEFINE THE NEW \"RAMP-AWARE\" LOSS\n",
        "# This is the \"Secret Sauce\" for your paper\n",
        "def ramp_aware_loss(y_true, y_pred):\n",
        "    # A. Standard Error\n",
        "    mse = tf.reduce_mean(tf.square(y_true - y_pred))\n",
        "\n",
        "    # B. The \"Panic\" Weight\n",
        "    # If the TRUE value is extreme (high or low spikes), weight the error x10\n",
        "    # We use distance from the mean (0.5) to find extremities\n",
        "    # This acts as a proxy for \"Ramp Events\" since ramps lead to extreme changes\n",
        "    volatility_weight = 1.0 + 10.0 * tf.cast(tf.abs(y_true - 0.5) > 0.2, tf.float32)\n",
        "\n",
        "    # C. Weighted Error\n",
        "    weighted_mse = tf.reduce_mean(tf.square(y_true - y_pred) * volatility_weight)\n",
        "\n",
        "    # D. Physics Constraints (Keep these!)\n",
        "    neg_penalty = tf.reduce_mean(tf.nn.relu(-y_pred))\n",
        "    upper_penalty = tf.reduce_mean(tf.nn.relu(y_pred - 1.2))\n",
        "\n",
        "    return weighted_mse + neg_penalty + upper_penalty\n",
        "\n",
        "# 3. LOAD & UPGRADE MODEL\n",
        "print(\"2. Upgrading Model with Ramp-Awareness...\")\n",
        "model = tf.keras.models.load_model(\"/content/drive/MyDrive/COLAB DATASET/final_physics_model.keras\", compile=False)\n",
        "\n",
        "# Re-compile with the NEW loss\n",
        "model.compile(optimizer=tf.keras.optimizers.Adam(learning_rate=0.0005), # Lower LR for fine-tuning\n",
        "              loss=ramp_aware_loss,\n",
        "              metrics=['mae'])\n",
        "\n",
        "# 4. FINE-TUNE (Retrain)\n",
        "# We train for 5 epochs just to teach it the new rules\n",
        "print(\"3. Retraining (Teaching the AI to panic)...\")\n",
        "history = model.fit(\n",
        "    [X_img_train, X_phys_train], y_train,\n",
        "    validation_data=([X_img_test, X_phys_test], y_test),\n",
        "    epochs=5,\n",
        "    batch_size=64,\n",
        "    verbose=1\n",
        ")\n",
        "\n",
        "# 5. SAVE THE UPGRADED MODEL\n",
        "model.save(\"/content/drive/MyDrive/COLAB DATASET/ramp_aware_model.keras\")\n",
        "print(\"✅ DONE. Model is now Ramp-Aware.\")"
      ],
