    {
      "cell_type": "code",
      "source": [
        "import numpy as np\n",
        "import pandas as pd\n",
        "import matplotlib.pyplot as plt\n",
        "from sklearn.metrics import mean_absolute_error\n",
        "\n",
        "# 1. FIND THE STORM (The most volatile day)\n",
        "print(\"1. Hunting for 'High Ramp' Days...\")\n",
        "df = pd.read_csv(\"/content/drive/MyDrive/COLAB DATASET/train_master_physics.csv\")\n",
        "\n",
        "# Calculate volatility (Daily Variance in Target K)\n",
        "# We want days where the power jumps up and down a lot\n",
        "df['date'] = pd.to_datetime(df['datetime']).dt.date\n",
        "daily_volatility = df.groupby('date')['target_k'].std()\n",
        "stormiest_day = daily_volatility.idxmax()\n",
        "\n",
        "print(f\"   🌪️ FOUND STORMIEST DAY: {stormiest_day}\")\n",
        "\n",
        "# 2. ISOLATE THAT DAY FOR TESTING\n",
        "mask = (df['date'] == stormiest_day)\n",
        "storm_indices = df.index[mask].values\n",
        "\n",
        "# Get the data for that specific day\n",
        "# We need to match indices to our Numpy Array\n",
        "# (Assuming aligned arrays from previous steps)\n",
        "start_idx = storm_indices[0]\n",
        "end_idx = storm_indices[-1]\n",
        "\n",
        "# Load Image Data (Assuming it's already loaded as X_img)\n",
        "# If not loaded, uncomment next line:\n",
        "# X_img = np.load(\"/content/drive/MyDrive/COLAB DATASET/X_images_64.npy\").astype('float32') / 255.0\n",
        "\n",
        "X_storm_img = X_img[start_idx:end_idx+1]\n",
        "y_storm_real = df['target_k'].iloc[start_idx:end_idx+1].values\n",
        "ghi_storm = df['GHI_limit'].iloc[start_idx:end_idx+1].values\n",
        "\n",
        "# Physics Input\n",
        "from sklearn.preprocessing import StandardScaler\n",
        "scaler = StandardScaler()\n",
        "X_storm_phys = scaler.fit_transform(ghi_storm.reshape(-1, 1))\n",
        "\n",
        "print(f\"   Testing on {len(y_storm_real)} minutes of Chaos...\")\n",
        "\n",
        "# 3. RUN PREDICTION\n",
        "y_storm_pred = model.predict([X_storm_img, X_storm_phys]).flatten()\n",
        "\n",
        "# 4. RE-CALCULATE RDI (Grid Safety)\n",
        "ramp_threshold = 0.10 # Lower threshold slightly for daily view\n",
        "actual_ramps = np.abs(np.diff(y_storm_real)) > ramp_threshold\n",
        "pred_ramps = np.abs(np.diff(y_storm_pred)) > ramp_threshold\n",
        "\n",
        "true_positives = np.sum(actual_ramps & pred_ramps)\n",
        "total_real_ramps = np.sum(actual_ramps)\n",
        "\n",
        "if total_real_ramps > 0:\n",
        "    rdi = true_positives / total_real_ramps\n",
        "else:\n",
        "    rdi = 0.0\n",
        "\n",
        "print(f\"\\n--- 📊 RESULTS FOR {stormiest_day} ---\")\n",
        "print(f\"   Total Ramp Events: {total_real_ramps}\")\n",
        "print(f\"   Detected by AI:    {true_positives}\")\n",
        "print(f\"   🏆 RDI Score:      {rdi:.2f}\")\n",
        "\n",
        "# 5. VISUAL PROOF\n",
        "plt.figure(figsize=(12, 6))\n",
        "plt.plot(y_storm_real, 'k-', linewidth=2, label='Actual Power (Volatile)')\n",
        "plt.plot(y_storm_pred, 'r--', linewidth=2, label='AI Prediction')\n",
        "plt.title(f'Stress Test: Performance on {stormiest_day}')\n",
        "plt.ylabel('Power Ratio')\n",
        "plt.legend()\n",
        "plt.grid(True)\n",
        "plt.show()"
      ],
