      "cell_type": "code",
      "source": [
        "import numpy as np\n",
        "import pandas as pd\n",
        "import matplotlib.pyplot as plt\n",
        "\n",
        "# 1. LOAD DATA\n",
        "df = pd.read_csv(\"/content/drive/MyDrive/COLAB DATASET/train_master_physics.csv\")\n",
        "df['date'] = pd.to_datetime(df['datetime']).dt.date\n",
        "\n",
        "# 2. FIX: FIND A \"PARTLY CLOUDY\" DAY (Bright Sun + Fast Clouds)\n",
        "# We calculate two metrics per day:\n",
        "# A. Max Power (Is it sunny?)\n",
        "daily_max = df.groupby('date')['target_k'].max()\n",
        "# B. Volatility (Are there clouds?)\n",
        "daily_std = df.groupby('date')['target_k'].std()\n",
        "\n",
        "# Filter: We only want days where power hit at least 80% (Sunny)\n",
        "# but also had high volatility (Clouds).\n",
        "sunny_days = daily_max[daily_max > 0.8].index\n",
        "# Find the most volatile among the sunny days\n",
        "stormiest_day = daily_std.loc[sunny_days].idxmax()\n",
        "\n",
        "print(f\"1. 🌪️ FOUND TRUE RAMP DAY: {stormiest_day}\")\n",
        "print(\"   (This day has bright sun AND crashing clouds)\")\n",
        "\n",
        "# 3. GET DATA FOR THAT DAY\n",
        "mask = (df['date'] == stormiest_day)\n",
        "storm_indices = df.index[mask].values\n",
        "start_idx = storm_indices[0]\n",
        "end_idx = storm_indices[-1]\n",
        "\n",
        "# Assuming X_img and X_phys are loaded/prepared from previous steps\n",
        "# (Reloading just to be safe for this block)\n",
        "X_img_all = np.load(\"/content/drive/MyDrive/COLAB DATASET/X_images_64.npy\").astype('float32') / 255.0\n",
        "X_storm_img = X_img_all[start_idx:end_idx+1]\n",
        "ghi_storm = df['GHI_limit'].iloc[start_idx:end_idx+1].values\n",
        "\n",
        "from sklearn.preprocessing import StandardScaler\n",
        "scaler = StandardScaler()\n",
        "X_storm_phys = scaler.fit_transform(ghi_storm.reshape(-1, 1))\n",
        "y_storm_real = df['target_k'].iloc[start_idx:end_idx+1].values\n",
        "\n",
        "# 4. PREDICT\n",
        "# Load model if not in memory (comment out if already loaded)\n",
        "# model = tf.keras.models.load_model(\"/content/drive/MyDrive/COLAB DATASET/final_physics_model.keras\", compile=False)\n",
        "y_storm_pred = model.predict([X_storm_img, X_storm_phys]).flatten()\n",
        "\n",
        "# 5. FIX: ADJUST THRESHOLD FOR 1-MINUTE DATA\n",
        "# A 15% jump in 1 minute is huge. Let's use 5% (0.05) which is standard for \"Minute-Scale Ramps\".\n",
        "ramp_threshold = 0.05\n",
        "\n",
        "actual_ramps = np.abs(np.diff(y_storm_real)) > ramp_threshold\n",
        "pred_ramps = np.abs(np.diff(y_storm_pred)) > ramp_threshold\n",
        "\n",
        "true_positives = np.sum(actual_ramps & pred_ramps)\n",
        "total_real_ramps = np.sum(actual_ramps)\n",
        "\n",
        "if total_real_ramps > 0:\n",
        "    rdi = true_positives / total_real_ramps\n",
        "else:\n",
        "    rdi = 0.0\n",
        "\n",
        "print(f\"\\n--- 📊 RESULTS FOR {stormiest_day} ---\")\n",
        "print(f\"   Ramp Threshold:    > {ramp_threshold*100}% per minute\")\n",
        "print(f\"   Total Ramp Events: {total_real_ramps}\")\n",
        "print(f\"   Detected by AI:    {true_positives}\")\n",
        "print(f\"   🏆 RDI Score:      {rdi:.2f}\")\n",
        "\n",
        "# 6. PLOT (The Evidence)\n",
        "plt.figure(figsize=(12, 6))\n",
        "plt.plot(y_storm_real, 'k-', linewidth=2, label='Actual Power')\n",
        "plt.plot(y_storm_pred, 'r--', linewidth=2, label='AI Prediction')\n",
        "plt.title(f'Ramp Detection Test on {stormiest_day}')\n",
        "plt.ylabel('Power Ratio (0-1)')\n",
        "plt.legend()\n",
        "plt.grid(True)\n",
        "plt.show()"
      ],
