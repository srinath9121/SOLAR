      "cell_type": "code",
      "source": [
        "# 1. INSTALL MISSING LIBRARIES (The Fix)\n",
        "!pip install pvlib\n",
        "!pip install datasets\n",
        "\n",
        "import os\n",
        "import numpy as np\n",
        "import pandas as pd\n",
        "from datasets import load_dataset\n",
        "from tqdm import tqdm\n",
        "from google.colab import drive\n",
        "from pvlib.location import Location\n",
        "\n",
        "# 2. MOUNT DRIVE\n",
        "if not os.path.exists('/content/drive'):\n",
        "    drive.mount('/content/drive')\n",
        "\n",
        "# 3. DEFINE SAVE PATHS (Permanent Storage)\n",
        "DRIVE_DIR = \"/content/drive/MyDrive/COLAB DATASET\"\n",
        "os.makedirs(DRIVE_DIR, exist_ok=True)\n",
        "\n",
        "# 4. LOAD & FILTER (2017 Only + Daylight)\n",
        "print(\"1. Streaming Data from Hugging Face...\")\n",
        "dataset = load_dataset(\"solarbench/skippd\", split=\"train\")\n",
        "\n",
        "# Extract Time & Power\n",
        "df = pd.DataFrame({'datetime': dataset['time'], 'power': dataset['pv']})\n",
        "\n",
        "# Convert Timezone (UTC -> Stanford)\n",
        "df['datetime'] = pd.to_datetime(df['datetime'])\n",
        "if df['datetime'].dt.tz is None:\n",
        "    df['datetime'] = df['datetime'].dt.tz_localize('UTC').dt.tz_convert('Etc/GMT+8')\n",
        "else:\n",
        "    df['datetime'] = df['datetime'].dt.tz_convert('Etc/GMT+8')\n",
        "\n",
        "# FILTER: 2017 + 9 AM to 4 PM\n",
        "print(\"2. Filtering for 2017 Daylight Hours...\")\n",
        "df = df.set_index('datetime')\n",
        "df = df[(df.index.year == 2017) & (df.index.hour >= 9) & (df.index.hour < 16)]\n",
        "\n",
        "# 5. PHYSICS CALCULATION (pvlib)\n",
        "print(\"3. Calculating Physics Limits...\")\n",
        "site = Location(37.42, -122.17, tz='Etc/GMT+8')\n",
        "clearsky = site.get_clearsky(df.index)\n",
        "\n",
        "df['GHI_limit'] = clearsky['ghi'].values\n",
        "df = df[df['GHI_limit'] > 10] # Remove dark periods\n",
        "df['target_k'] = (df['power'] / df['GHI_limit']).clip(0, 1.2)\n",
        "\n",
        "# Save Physics CSV directly to Drive\n",
        "csv_path = os.path.join(DRIVE_DIR, \"train_master_physics.csv\")\n",
        "df.reset_index().to_csv(csv_path, index=False)\n",
        "print(f\"✅ Saved Physics CSV to Drive: {len(df)} rows\")\n",
        "\n",
        "# 6. PROCESS IMAGES & SAVE\n",
        "print(\"4. Processing Matching Images (Resize 64x64)...\")\n",
        "valid_times = set(df.index.astype(str))\n",
        "processed_images = []\n",
        "\n",
        "for sample in tqdm(dataset):\n",
        "    try:\n",
        "        ts = pd.to_datetime(sample['time'])\n",
        "        if ts.tz is None: ts = ts.tz_localize('UTC').tz_convert('Etc/GMT+8')\n",
        "        else: ts = ts.tz_convert('Etc/GMT+8')\n",
        "\n",
        "        if str(ts) in valid_times:\n",
        "            img = sample['image'].resize((64, 64))\n",
        "            processed_images.append(np.array(img, dtype=np.uint8))\n",
        "    except: continue\n",
        "\n",
        "# Save Images directly to Drive\n",
        "npy_path = os.path.join(DRIVE_DIR, \"X_images_64.npy\")\n",
        "np.save(npy_path, np.array(processed_images))\n",
        "print(f\"✅ Saved {len(processed_images)} Images to Drive!\")\n",
        "print(\"🎉 RECOVERY COMPLETE. Your 2017 Thesis Data is safe.\")"
      ],
