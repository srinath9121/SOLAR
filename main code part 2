    {
      "cell_type": "code",
      "source": [
        "import numpy as np\n",
        "import pandas as pd\n",
        "import matplotlib.pyplot as plt\n",
        "from sklearn.preprocessing import StandardScaler\n",
        "\n",
        "# 1. SETUP\n",
        "print(\"1. Loading Data for the Final Stress Test...\")\n",
        "df = pd.read_csv(\"/content/drive/MyDrive/COLAB DATASET/train_master_physics.csv\")\n",
        "df['date'] = pd.to_datetime(df['datetime']).dt.date\n",
        "\n",
        "# 2. TARGET THE SPECIFIC DAY (Oct 30, 2017)\n",
        "target_date = pd.to_datetime(\"2017-10-30\").date()\n",
        "print(f\"   🎯 Locking onto Target: {target_date}\")\n",
        "\n",
        "mask = (df['date'] == target_date)\n",
        "indices = df.index[mask].values\n",
        "\n",
        "if len(indices) == 0:\n",
        "    print(\"❌ ERROR: No data found for this date. Check your CSV.\")\n",
        "else:\n",
        "    start_idx = indices[0]\n",
        "    end_idx = indices[-1]\n",
        "    print(f\"   Found {len(indices)} minutes of data.\")\n",
        "\n",
        "    # 3. PREPARE INPUTS\n",
        "    # We load the images for just this slice (to save RAM)\n",
        "    # Note: We load the whole file then slice, as .npy doesn't support partial loading easily\n",
        "    X_all = np.load(\"/content/drive/MyDrive/COLAB DATASET/X_images_64.npy\").astype('float32') / 255.0\n",
        "    X_storm = X_all[start_idx:end_idx+1]\n",
        "\n",
        "    # Physics Input\n",
        "    ghi_storm = df['GHI_limit'].iloc[start_idx:end_idx+1].values\n",
        "    scaler = StandardScaler()\n",
        "    X_phys_storm = scaler.fit_transform(ghi_storm.reshape(-1, 1))\n",
        "\n",
        "    # Real Values\n",
        "    y_real = df['target_k'].iloc[start_idx:end_idx+1].values\n",
        "\n",
        "    # 4. RUN PREDICTION\n",
        "    # Load model if needed (uncomment if not loaded)\n",
        "    # import tensorflow as tf\n",
        "    # model = tf.keras.models.load_model(\"/content/drive/MyDrive/COLAB DATASET/final_physics_model.keras\", compile=False)\n",
        "\n",
        "    print(\"2. Running AI Prediction...\")\n",
        "    y_pred = model.predict([X_storm, X_phys_storm]).flatten()\n",
        "\n",
        "    # 5. CALCULATE RDI (The Thesis Metric)\n",
        "    # We use the 5% threshold identified by your audit\n",
        "    ramp_threshold = 0.05\n",
        "\n",
        "    # Calculate drops/jumps\n",
        "    real_ramps = np.abs(np.diff(y_real)) > ramp_threshold\n",
        "    pred_ramps = np.abs(np.diff(y_pred)) > ramp_threshold\n",
        "\n",
        "    # Statistics\n",
        "    total_events = np.sum(real_ramps)\n",
        "    caught_events = np.sum(real_ramps & pred_ramps)\n",
        "\n",
        "    if total_events > 0:\n",
        "        rdi = caught_events / total_events\n",
        "    else:\n",
        "        rdi = 0.0\n",
        "\n",
        "    print(f\"\\n--- 🏆 FINAL SAFETY SCORES ({target_date}) ---\")\n",
        "    print(f\"   Ramp Threshold:    5% change per minute\")\n",
        "    print(f\"   Total Ramp Events: {total_events} (Should be ~10)\")\n",
        "    print(f\"   Caught by AI:      {caught_events}\")\n",
        "    print(f\"   ✅ Ramp Detection Index (RDI): {rdi:.2f}\")\n",
        "\n",
        "    # 6. GENERATE THE EVIDENCE PLOT\n",
        "    plt.figure(figsize=(14, 6))\n",
        "    plt.plot(y_real, 'k-', linewidth=2, label='Actual Power (Volatile)')\n",
        "    plt.plot(y_pred, 'r--', linewidth=2, label='Physics-AI Forecast')\n",
        "\n",
        "    # Highlight the ramps\n",
        "    ramp_locs = np.where(real_ramps)[0]\n",
        "    plt.scatter(ramp_locs, y_real[ramp_locs], color='orange', s=100, zorder=5, label='Ramp Event (>5%)')\n",
        "\n",
        "    plt.title(f\"Stress Test: Ramp Detection on {target_date}\")\n",
