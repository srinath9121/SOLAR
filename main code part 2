    {
      "cell_type": "code",
      "source": [
        "import numpy as np\n",
        "import pandas as pd\n",
        "import tensorflow as tf\n",
        "import matplotlib.pyplot as plt\n",
        "from sklearn.preprocessing import StandardScaler\n",
        "\n",
        "# --- 1. PREPARE THE \"FORCE-FED\" DATA ---\n",
        "print(\"1. Preparing 'Force-Fed' Training Data...\")\n",
        "\n",
        "# Load Data\n",
        "X_img = np.load(\"/content/drive/MyDrive/COLAB DATASET/X_images_64.npy\").astype('float32') / 255.0\n",
        "df = pd.read_csv(\"/content/drive/MyDrive/COLAB DATASET/train_master_physics.csv\")\n",
        "\n",
        "# Align\n",
        "min_len = min(len(X_img), len(df))\n",
        "X_img = X_img[:min_len]\n",
        "y = df['target_k'].iloc[:min_len].values\n",
        "ghi = df['GHI_limit'].iloc[:min_len].values\n",
        "\n",
        "# Split (Chronological)\n",
        "split = int(len(df) * 0.8)\n",
        "X_img_train = X_img[:split]\n",
        "y_train = y[:split]\n",
        "scaler = StandardScaler()\n",
        "X_phys_train = scaler.fit_transform(ghi[:split].reshape(-1, 1))\n",
        "\n",
        "# IDENTIFY RAMPS (The \"Needle in the Haystack\")\n",
        "# We look for jumps > 5% in the Training Set\n",
        "is_ramp = np.abs(np.diff(y_train, append=0)) > 0.05\n",
        "ramp_count = np.sum(is_ramp)\n",
        "print(f\"   Found {ramp_count} ramp events in the original training set.\")\n",
        "\n",
        "# DUPLICATE THEM (The \"Force Feed\")\n",
        "# We multiply these specific samples by 50 so the AI sees them constantly\n",
        "X_img_ramps = X_img_train[is_ramp]\n",
        "X_phys_ramps = X_phys_train[is_ramp]\n",
        "y_ramps = y_train[is_ramp]\n",
        "\n",
        "print(\"   🚀 Boosting Ramps by 50x...\")\n",
        "X_img_boosted = np.concatenate([X_img_train] + [X_img_ramps] * 50, axis=0)\n",
        "X_phys_boosted = np.concatenate([X_phys_train] + [X_phys_ramps] * 50, axis=0)\n",
        "y_boosted = np.concatenate([y_train] + [y_ramps] * 50, axis=0)\n",
        "\n",
        "# SHUFFLE (Crucial so they mix in)\n",
        "indices = np.arange(len(y_boosted))\n",
        "np.random.shuffle(indices)\n",
        "X_img_final = X_img_boosted[indices]\n",
        "X_phys_final = X_phys_boosted[indices]\n",
        "y_final = y_boosted[indices]\n",
        "\n",
        "print(f\"   Training Set Size: {len(y_train)} -> {len(y_final)} (Includes Fake Copies)\")\n",
        "\n",
        "# --- 2. RETRAIN THE MODEL ---\n",
        "print(\"\\n2. Retraining on Boosted Data...\")\n",
        "model = tf.keras.models.load_model(\"/content/drive/MyDrive/COLAB DATASET/ramp_aware_model.keras\", compile=False)\n",
        "\n",
        "# Compile with a slightly higher learning rate to break the \"polite\" habits\n",
        "model.compile(optimizer=tf.keras.optimizers.Adam(learning_rate=0.001), loss='mse', metrics=['mae'])\n",
        "\n",
        "model.fit(\n",
        "    [X_img_final, X_phys_final], y_final,\n",
        "    epochs=5,\n",
        "    batch_size=64,\n",
        "    verbose=1\n",
        ")\n",
        "\n",
        "# --- 3. IMMEDIATE STRESS TEST (OCT 30) ---\n",
        "print(\"\\n3. Testing on Oct 30 (The Storm Day)...\")\n",
        "\n",
        "# Get Test Data (Oct 30)\n",
        "df['date'] = pd.to_datetime(df['datetime']).dt.date\n",
        "target_date = pd.to_datetime(\"2017-10-30\").date()\n",
        "mask = (df['date'] == target_date)\n",
        "start_idx, end_idx = df.index[mask].values[0], df.index[mask].values[-1]\n",
        "\n",
        "X_storm = X_img[start_idx:end_idx+1]\n",
        "y_real_storm = y[start_idx:end_idx+1]\n",
        "ghi_storm = df['GHI_limit'].iloc[start_idx:end_idx+1].values\n",
        "X_phys_storm = scaler.transform(ghi_storm.reshape(-1, 1)) # Use same scaler\n",
        "\n",
        "# Predict\n",
        "y_pred_storm = model.predict([X_storm, X_phys_storm]).flatten()\n",
        "\n",
        "# Score\n",
        "threshold = 0.05\n",
        "real_ramps = np.abs(np.diff(y_real_storm)) > threshold\n",
        "pred_ramps = np.abs(np.diff(y_pred_storm)) > threshold\n",
        "\n",
        "caught = np.sum(real_ramps & pred_ramps)\n",
        "total = np.sum(real_ramps)\n",
        "rdi = caught / total if total > 0 else 0\n",
        "\n",
        "print(f\"\\n--- 🏆 FORCE-FEED RESULTS ---\")\n",
        "print(f\"   Caught: {caught} / {total}\")\n",
        "print(f\"   RDI:    {rdi:.2f}\")\n",
        "\n",
        "# Plot\n",
        "plt.figure(figsize=(14, 6))\n",
        "plt.plot(y_real_storm, 'k-', linewidth=2, label='Actual')\n",
        "plt.plot(y_pred_storm, 'r--', linewidth=2, label='AI Forecast')\n",
        "hits = np.where(real_ramps & pred_ramps)[0]\n",
        "plt.scatter(hits, y_real_storm[hits], color='lime', s=200, zorder=5, label='Caught!')\n",
        "plt.title(f\"Force-Fed Model (RDI: {rdi:.2f})\")\n",
        "plt.legend()\n",
        "plt.show()"
      ],
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/"
        },
        "id": "3yN0tLb7D2v5",
        "outputId": "cdec5256-5775-4da9-d073-9b1322341ebf"
      },
      "execution_count": null,
      "outputs": [
        {
          "output_type": "stream",
          "name": "stdout",
