      "cell_type": "code",
      "source": [
        "import pandas as pd\n",
        "import numpy as np\n",
        "import matplotlib.pyplot as plt\n",
        "\n",
        "# 1. LOAD DATA\n",
        "print(\"1. Loading Dataset...\")\n",
        "file_path = \"/content/drive/MyDrive/COLAB DATASET/train_master_physics.csv\"\n",
        "df = pd.read_csv(file_path)\n",
        "\n",
        "# Ensure datetime is correct\n",
        "df['datetime'] = pd.to_datetime(df['datetime'])\n",
        "df = df.sort_values('datetime')\n",
        "\n",
        "# 2. CALCULATE RAMPS (The Math)\n",
        "# A \"Ramp\" is the absolute change in Power Ratio (target_k) from one minute to the next.\n",
        "df['ramp_magnitude'] = df['target_k'].diff().abs()\n",
        "\n",
        "# 3. COUNT EVENTS (Industry Standards)\n",
        "# > 5% change in 1 minute is significant\n",
        "# > 10% change in 1 minute is extreme\n",
        "count_5_percent = (df['ramp_magnitude'] > 0.05).sum()\n",
        "count_10_percent = (df['ramp_magnitude'] > 0.10).sum()\n",
        "\n",
        "print(f\"\\n--- 📊 DATASET AUDIT RESULTS ---\")\n",
        "print(f\"Total Minutes Scanned: {len(df)}\")\n",
        "print(f\"Ramp Events (> 5% / min):  {count_5_percent}\")\n",
        "print(f\"Extreme Events (> 10% / min): {count_10_percent}\")\n",
        "\n",
        "# 4. FIND THE \"PERFECT STORM\" (The best day to test your model)\n",
        "# We group by Date and count how many >5% ramps happened that day.\n",
        "df['date'] = df['datetime'].dt.date\n",
        "daily_ramp_counts = df[df['ramp_magnitude'] > 0.05].groupby('date').size()\n",
        "\n",
        "if not daily_ramp_counts.empty:\n",
        "    worst_day = daily_ramp_counts.idxmax()\n",
        "    worst_count = daily_ramp_counts.max()\n",
        "\n",
        "    print(f\"\\n🌪️ BEST TEST DAY: {worst_day}\")\n",
        "    print(f\"   (This day had {worst_count} major ramp events)\")\n",
        "\n",
        "    # 5. VISUALIZE THAT DAY\n",
        "    # Plotting this day proves to the professor that the data is volatile.\n",
        "    day_data = df[df['date'] == worst_day]\n",
        "\n",
        "    plt.figure(figsize=(12, 5))\n",
        "    plt.plot(day_data['datetime'], day_data['target_k'], color='black', linewidth=1.5)\n",
        "    plt.title(f\"Volatility Profile: {worst_day} ({worst_count} Ramps)\")\n",
        "    plt.ylabel(\"Power Ratio (target_k)\")\n",
        "    plt.xlabel(\"Time\")\n",
        "    plt.grid(True, alpha=0.3)\n",
        "    plt.show()\n",
        "\n",
        "else:\n",
        "    print(\"\\n⚠️ NO RAMP EVENTS FOUND. Check your target_k calculation.\")"
      ],
