        "import os\n",
        "import shutil\n",
        "import numpy as np\n",
        "import pandas as pd\n",
        "from google.colab import drive\n",
        "\n",
        "# 1. MOUNT GOOGLE DRIVE\n",
        "print(\"1. Connecting to Google Drive...\")\n",
        "if not os.path.exists('/content/drive'):\n",
        "    drive.mount('/content/drive')\n",
        "\n",
        "# 2. DEFINE YOUR SAFE FOLDER\n",
        "# This is where your thesis data will live permanently\n",
        "DRIVE_DIR = \"/content/drive/MyDrive/COLAB DATASET\"\n",
        "os.makedirs(DRIVE_DIR, exist_ok=True)\n",
        "\n",
        "# 3. LOCATE YOUR PROCESSED DATA\n",
        "# (Based on the code you ran in untitled28.py)\n",
        "local_images = \"data/processed/X_images_64.npy\"\n",
        "local_physics = \"/content/drive/MyDrive/COLAB DATASET/final_physics_model.keras\"\n",
        "\n",
        "# 4. SAVE TO DRIVE (The Critical Step)\n",
        "print(f\"2. Saving files to: {DRIVE_DIR}\")\n",
        "\n",
        "# A. Save Images\n",
        "if os.path.exists(local_images):\n",
        "    target_img = os.path.join(DRIVE_DIR, \"X_images_64.npy\")\n",
        "    print(f\"   Copying Images... (This may take 1-2 mins)\")\n",
        "    shutil.copy(local_images, target_img)\n",
        "    print(\"   ✅ Images Saved!\")\n",
        "else:\n",
        "    print(\"   ⚠️ WARNING: 'X_images_64.npy' not found locally. Did the processing finish?\")\n",
        "\n",
        "# B. Save Physics CSV\n",
        "if os.path.exists(local_physics):\n",
        "    target_csv = os.path.join(DRIVE_DIR, \"train_master_physics.csv\")\n",
        "    print(f\"   Copying Physics Data...\")\n",
        "    shutil.copy(local_physics, target_csv)\n",
        "    print(\"   ✅ Physics Data Saved!\")\n",
        "else:\n",
